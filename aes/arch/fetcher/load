    %vpi_call/w 4 13 "$finish" {0 0 0};
T_0.0 ;
    %end;
    .thread T_0;
    .scope S_0x15b705b40;
T_1 ;
    %wait E_0x15b70a0a0;
    %vpi_func 4 18 "$feof" 32, v0x15b71d860_0 {0 0 0};
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %vpi_func 4 19 "$fgets" 32, v0x15b71d7c0_0, v0x15b71d860_0 {0 0 0};
    %store/vec4 v0x15b71d910_0, 0, 32;
    %jmp T_1.1;
T_1.0 ;
    %vpi_call/w 4 23 "$fclose", v0x15b71d860_0 {0 0 0};
    %vpi_call/w 4 24 "$finish" {0 0 0};
    %pushi/vec4 0, 0, 4096;
    %store/vec4 v0x15b71d7c0_0, 0, 4096;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x15b7059d0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x15b71d9f0_0, 0, 1;
T_2.0 ;
    %delay 5, 0;
