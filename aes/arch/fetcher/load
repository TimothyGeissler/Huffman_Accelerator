v0x15b71d860_0 .var/i "file_faults", 31 0;
v0x15b71d910_0 .var/i "scan_faults", 31 0;
E_0x15b70a0a0 .event posedge, v0x15b70d8b0_0;
    .scope S_0x15b705b40;
T_0 ;
    %vpi_func 4 10 "$fopen" 32, "load_data.txt", "r" {0 0 0};
    %store/vec4 v0x15b71d860_0, 0, 32;
